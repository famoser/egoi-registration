{
  "type": "project",
  "name": "famoser/egoi-registration",
  "description": "registration tool for egio",
  "license": "MIT",
  "require": {
    "php": ">=7.2.5",
    "ext-ctype": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "composer/package-versions-deprecated": "^1.11",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.7",
    "nelmio/cors-bundle": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.2",
    "sensio/framework-extra-bundle": "^5.6",
    "symfony/asset": "^v5.1.11",
    "symfony/console": "^v5.1.11",
    "symfony/dotenv": "^v5.1.11",
    "symfony/expression-language": "^v5.1.11",
    "symfony/filesystem": "^v5.1.11",
    "symfony/flex": "^1.3.1",
    "symfony/form": "^v5.1.11",
    "symfony/framework-bundle": "^v5.1.11",
    "symfony/mailer": "^v5.1.11",
    "symfony/monolog-bundle": "^3.5",
    "symfony/property-access": "^v5.1.11",
    "symfony/property-info": "^v5.1.11",
    "symfony/requirements-checker": "^1.1",
    "symfony/security-bundle": "^v5.1.11",
    "symfony/serializer": "^v5.1.11",
    "symfony/translation": "^v5.1.11",
    "symfony/twig-bundle": "^v5.1.11",
    "symfony/uid": "^v5.1.11",
    "symfony/validator": "^v5.1.11",
    "symfony/webpack-encore-bundle": "^1.7",
    "symfony/yaml": "^v5.1.11",
    "twig/cssinliner-extra": "^3.0",
    "twig/extra-bundle": "^3.0",
    "twig/inky-extra": "^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": "^2.8",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "famoser/agnes": "^4.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "liip/functional-test-bundle": "^4.2",
    "liip/test-fixtures-bundle": "^1.10.0-alpha",
    "symfony/browser-kit": "^5.1",
    "symfony/css-selector": "^5.1",
    "symfony/debug-bundle": "^5.1",
    "symfony/maker-bundle": "^1.21",
    "symfony/phpunit-bridge": "^5.1",
    "symfony/stopwatch": "^5.1",
    "symfony/var-dumper": "^5.1",
    "symfony/web-profiler-bundle": "^5.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "requirements-checker": "script"
    },
    "prepare-environment": [
      "touch .env.local",
      "vendor/bin/cghooks update",
      "@fixtures"
    ],
    "fixtures": [
      "php bin/console doctrine:migrations:migrate  --no-interaction",
      "php bin/console doctrine:fixtures:load --no-interaction"
    ],
    "create-migration": [
      "php bin/console doctrine:migrations:diff --no-interaction"
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@prepare-environment"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "@prepare-environment"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^v5.1.11"
    },
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "./vendor/bin/php-cs-fixer fix",
        "yarn run lint-fix",
        "git update-index --again"
      ],
      "post-merge": "composer install"
    }
  }
}
